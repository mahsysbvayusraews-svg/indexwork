{
  "version": 2,
  "framework": null,
  "builds": [
    {"src": "index.php", "use": "vercel-php@0.6.2"},
    {"src": "otp.php", "use": "vercel-php@0.6.2"},
    {"src": "config.php", "use": "vercel-php@0.6.2"}
  ],
  "routes": [
    {"src": "/assets/(.*)", "dest": "/assets/$1"},
    {"src": "/otp", "dest": "/otp.php"},
    {"src": "/(.*)", "dest": "/index.php"}
  ],
  "env": {"PHP_VERSION": "8.1"}
}